// Hex Colors
hex-teal        = #039499
hex-dk-teal     = #006b71
hex-lght-yellow = #f4b937
hex-lght-red    = #ed573f

// RGB Colors
rgb-teal        = 3,148,153

// Color Classes
.color-white
    color #fff

// Override Classes
.container
  padding-left 15px
  padding-right 15px

.well 
  margin-bottom 0px
  padding 0px

.full
  display block
  width 100%
  height 100%

.no-style
  background-color transparent !important
  background-image none !important
  border none !important
  list-style none !important
  text-decoration none !important
  border none !important
  border-radius 0px !important
  -webkit-box-shadow none !important
  box-shadow none !important

.no-padding
  padding 0px !important

.no-margin
  margin 0px !important

.transition-all
    -webkit-transition all 0.4s ease
    -moz-transition all 0.4s ease
    -o-transition all 0.4s ease
    transition all 0.4s ease

.transition-background
    -webkit-transition background 0.4s ease
    -moz-transition background 0.4s ease
    -o-transition background 0.4s ease
    transition background 0.4s ease

// User Agent Visibility Overrides
// Detect iOS and Android for hidden and visible classes used with device-detect.js
.hidden-android
    display none

.hidden-ios
    display none

.hidden-web
    display none

.visible-web
    display block

// Global Styles
body
    padding-top 60px
    background-color #e2e2e2 !important
    font-family 'Lato', Helvetica, sans-serif

h1, h2, h3, h4, h5, h6, p, a, input, button, select, textarea, span
    font-family 'Lato', Helvetica, sans-serif

// Start Styles
// Panel Get Updates
#panel-updates
    float left
    position fixed
    right 0
    left 0
    z-index 1030
    height 180px
    width 100%
    background-color #333
    z-index 1000
    -moz-box-shadow inset 0 -2px 4px rgba(0,0,0,.3)
    -webkit-box-shadow inset 0 -2px 4px rgba(0,0,0,.3)
    box-shadow inset 0 -2px 4px rgba(0,0,0,.3)

.panel-updates-closed
    top -180px

.panel-updates-opened
    display block
    top 0

#panel-updates-h1
    margin-top 15px

.input-group
    width 100%

.form-control
    -moz-border-radius 4px
    -webkit-border-radius 4px
    border-radius 4px
    min-height 52px

#input-email
    width 100%
    
#input-zip
    width 100%

#btn-updates-submit,
#mc-embedded-subscribe
    width 100%
    height 52px
    -moz-border-radius 4px
    -webkit-border-radius 4px
    border-radius 4px

#mc-embedded-subscribe
    background-color hex-teal
    color #fff
    padding 0
    border 1px solid hex-dk-teal

#close-panel-updates
    color #fff
    width 100%
    font-size 36px
    line-height 36px
    text-align right
    margin-top 15px
    cursor pointer

// Global Navigation
.navbar
    background-color rgba(hex-teal,.75)
    margin-bottom 0px
    border none

.navbar-fixed-top
    border-width 0

.navbar-fixed-top-panel-opened
    position fixed
    top 180px
    right 0
    left 0
    z-index 1030
    border-width 0
    border-radius 0

#container-nav-links
    position absolute
    min-height 60px

.navbar-nav,
.navbar-nav>li,
.navbar-nav>li>a
    min-height 60px

.navbar-nav>li>a
    text-align left
    padding 0 15px 0 0
    line-height 60px
    text-shadow none
    color #fff
    font-size 16px
    font-weight 300

.nav>li>a:hover,
.nav>li>a:focus
    color hex-lght-yellow
    text-decoration none
    background-color transparent

.navbar-header
    width 100%
    margin-left 15px

#container-brand
    float left
    width 100%
    position initial
    top 0
    min-height 60px

.navbar-brand
    width 100%
    min-height 60px
    display block
    padding 0px
    text-indent -9999px
    background-image url(../img/lincoln-labs-logo.png)
    background-repeat no-repeat 
    background-position center center

.navbar-btn-updates
    width 100%
    min-height 38px
    display block
    text-align center
    margin-top 10px
    color #fff
    font-size 16px
    font-weight 300
    line-height 38px
    border 1px solid #fff
    -moz-border-radius 4px
    -webkit-border-radius 4px
    border-radius 4px

.navbar-btn-updates:hover
    color hex-lght-yellow
    background-color hex-dk-teal
    border-color hex-lght-yellow

.navbar-btn-updates:hover
    text-decoration none

.nav .open>a, 
.nav .open>a:hover, 
.nav .open>a:focus
    color hex-lght-yellow
    background-color transparent
    border-color none

.dropdown-menu
    background-color hex-teal
    border none
    border-top 1px solid #039499
    -webkit-box-shadow 0 2px 2px rgba(0,0,0,0.175)
    box-shadow 0 2px 2px rgba(0,0,0,0.175)
    -moz-box-shadow inset 0 2px 4px rgba(0,0,0,.3)
    -webkit-box-shadow inset 0 2px 4px rgba(0,0,0,.3)
    box-shadow inset 0 2px 4px rgba(0,0,0,.3)

.dropdown-menu>li>a, 
.dropdown-menu>li>a
    color #fff

.dropdown-menu>li>a:hover, 
.dropdown-menu>li>a:focus
    color hex-lght-yellow
    background-image none
    background-color transparent

.navbar-toggle
    border 1px solid #fff
    min-height 38px
    margin-top 10px
    margin-right 0px
    width 100%

.navbar-toggle .icon-bar
    border-top 1px solid #fff
    border-bottom 1px solid #fff
    width 100%

.navbar-collapse
    padding-left 0

// Homepage Content
#bg-body-homepage
