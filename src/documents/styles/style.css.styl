#sponsor-g-o a,
#sponsor-microsoft a
    display block
    width 100%
    height 100%
    text-indent -9999px
    
#sponsor-microsoft
    display block
    width 100%
    height 120px
    background-image url(../img/microsoft.jpg)
    background-repeat no-repeat
    background-position center center
    background-size 50%

#sponsor-g-o
    display block
    width 100%
    height 120px
    background-image url(../img/GenOpp-Logo.png)
    background-repeat no-repeat
    background-position center center
