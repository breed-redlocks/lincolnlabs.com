// Extra Small devices (up to 479px)
@media (max-width: 479px)
    
    // Panel Get Updates
    #panel-updates
        top -180px
        visibility hidden
        -webkit-transition none
        -moz-transition none
        -o-transition none
        transition none
    
    // Global Navigation
    #navbar,
    .navbar,
    .navbar-fixed-top-panel-opened,
    .animation-navbar-open
        top 0px !important

    .navbar-header
        margin-left 0px
        padding-left 0px

    #container-nav-links
        min-height 0px

    .navbar-brand
        background-position left center
        display block !important

    .navbar-collapse
        padding-left 15px

    .in,
    .collapsing
        background-color rgba(hex-teal,.9)
        top 60px
        width 100%

    .in>ul,
    .collapsing>ul
        padding-left 15px
        padding-right 15px

    // Homepage Content
    #img-lincoln-labs-logo-full
        height 304px
        background-size 136px 126px
        background-position center 45px

    #homepage-h2
        padding-top 190px
        font-size 22px
        font-weight 500

    #panel-updates-h1
        margin-top 30px
        color hex-dk-teal

    #mc-embedded-subscribe
        margin-top 15px

    .homepage-goal
        min-height 450px;
        
    .homepage-h5, 
    .homepage-goal-p
        margin-top 60px

// Medium Small devices (tablets, 480px and up)
@media (min-width: 480px) and (max-width: 767px)
    
    // Global Navigation
    .navbar-header
        margin-left 0px

    #container-nav-links
        min-height 0px

    .navbar-brand
        background-position left center
        display block !important

    .navbar-btn-updates
        font-size 14px

    .navbar-collapse
        padding-left 15px

    .in,
    .collapsing
        background-color rgba(hex-teal,.9)
        top 60px
        width 100%

    .in>ul,
    .collapsing>ul
        padding-left 15px
        padding-right 15px


