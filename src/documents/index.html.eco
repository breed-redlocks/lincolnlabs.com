        <%- @partial('email_form') %>
      </div>
    </div>

    <div class="row" id="homepage-headline-about">
      <div class="col-xs-12 col-ms-12 col-sm-12 col-md-12 col-lg-12">

        <a href="/conferences/reboot/hackathon">
          <h1 class="homepage-h1">Reboot Hackathon</h1>
          <h2 id="build-tools" class="homepage-h2">Build the Right Tools</h2>
          <h3 class="homepage-h3 bold prize-money">$10,000 Prize</h3> <!-- smaller font, but bold -->
        </a>

